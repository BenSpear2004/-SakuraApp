<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="66.0" prefWidth="480.0" styleClass="song-row" stylesheets="@../CSS/CheckOutCard.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.utsa.cs3743.sakuraapplication.Controller.CartItemCardController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="66.0" prefWidth="474.0" spacing="10.0">
         <children>
            <ImageView fx:id="foodImage" fitHeight="65.0" fitWidth="78.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets left="7.0" />
               </HBox.margin></ImageView>
            <Label fx:id="nameLabel" alignment="CENTER" prefHeight="20.0" prefWidth="195.0" styleClass="nameLabel" text="Name" textAlignment="RIGHT" />
            <Label fx:id="quantityLabel" alignment="CENTER" prefHeight="20.0" prefWidth="150.0" styleClass="QuantityLabel" text="Quantiy" />
            <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="96.0" styleClass="RemoveBTN" text="Remove" />
         </children>
      </HBox>
   </children>
</AnchorPane>
